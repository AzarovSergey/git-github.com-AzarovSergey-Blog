@using MvcPL.Models
@{
    Layout = @"~/Views/Shared/_Layout.cshtml";
}


@using MvcPL.Models
@using MvcPL.Controllers


@{
    if (TempData.ContainsKey(ArticleController.articleKey))
    {
        Html.RenderPartial(@"~/Views/Article/_ArticlesDescriptionList.cshtml", TempData[ArticleController.articleKey]);
        TempData.Remove(ArticleController.articleKey);
    }
}
